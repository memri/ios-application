{
    "{type:Note}": {
        "name": "all-notes",
        "title": "Notes",
        "searchResult": {
            "query": "data:Note",
            "sortProperty": "date-created",
            "sortAscending": 0
        },
        "rendererName": "list",
        "renderConfig": {
            "list": {
                
            }
        },
        "browsingMode": "default",
        "editButtons": [
            {
                "icon": "add.ico",
                "title": "Add Note",
                "actionName": "add",
                "actionArgs": [{
                    "type": "note",
                    "predicates": {"owner": "{me}"},
                    "properties": {"title": "Untitled Note"}
                }]
            }
        ],
        "filterButtons": [
            {
                "icon": "star.ico",
                "title": "Show Starred Notes",
                "actionName": "showStarred",
                "actionArgs": []
            },
            {
                "icon": "filter.ico",
                "title": "Show Filter Panel",
                "actionName": "showFilterPane",
                "actionArgs": []
            }
        ],
        "contextButtons": [
            {
                "icon": "star.ico",
                "title": "Star Note",
                "actionName": "star",
                "actionArgs": ["{.id}"]
            },
            {
                "icon": "schedule.ico",
                "title": "Schedule...",
                "actionName": "schedule",
                "actionArgs": []
            }
        ],
        "actionItems": [
            {
                "title": "Share with...",
                "actionName": "showOverlay",
                "actionArgs": ["shareWithPanel"]
            },
            {
                "title": "Add to list...",
                "actionName": "addToPanel",
                "actionArgs": []
            },
            {
                "title": "Duplicate Note",
                "actionName": "duplicate",
                "actionArgs": ["{@.id}"]
            }
        ],
        "navigateItems": [
            {
                "title": "Timeline of this note",
                "actionName": "openView",
                "actionArgs": [{
                    "rendererName": "timeline",
                    "searchResult": {
                        "query": "data:Log appliesTo:{.id}",
                        "sortProperty": "date-created",
                        "sortAscending": 0
                    }
                }]
            },
            {
                "title": "Starred notes",
                "actionName": "showStarred",
                "actionArgs": []
            },
            {
                "title": "All notes",
                "actionName": "openView",
                "actionArgs": ["all-notes"]
            }
        ]
    }
}