[
    {
        "name": "filter-starred",
        "fromTemplate": "{view}",
        "queryOptions": {
            "query": "{computedView.queryOptions.query} AND starred = true"
        },
        "title": "Starred {computedView.title}"
    },
    
    {
        "name": "shopping-notes",
        "views": [
            "all-notes",
            {
                "title": "Shopping Notes",
                "queryOptions": {
                    "query": "note AND title BEGINSWITH[c] 'shopping'"
                }
            }
        ]
    },
    {
        "name": "all-people",
        "views": [
            {
                "title": "All people",
                "queryOptions": {
                    "query": "person"
                }
            }
        ]
    }
]
